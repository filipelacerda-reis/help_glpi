    "postinstall": "prisma generate",
    "kill-port": "powershell -ExecutionPolicy Bypass -File ./scripts/kill-port.ps1 -Port 8080",
    "check-db": "powershell -ExecutionPolicy Bypass -File ./scripts/check-db.ps1",
    "workers": "tsx src/workers/start-workers.ts",
    "workers:dev": "tsx watch src/workers/start-workers.ts"
  },
  "dependencies": {
